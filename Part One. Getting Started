(6 rows)

joins_exercise=# SELECT first_name, last_name, ROUND(AVG(price)) as average_prices, COUNT(*) FROM owners JOIN vehicles ON owners.id = vehicles.owner_id GROUP BY (first_name, last_name) HAVING COUNT(*) > 1 AND ROUND(AVG(price)) > 10000 ORDER BY first_name DESC;
